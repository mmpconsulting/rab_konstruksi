<odoo>
    <data>
        <record id="seq_bp_pr_purchase_request" model="ir.sequence">
            <field name="name">Purchase Request</field>
            <field name="code">bp_pr.purchase_request</field>
            <field name="prefix">PR-%(y)s%(month)s</field>
